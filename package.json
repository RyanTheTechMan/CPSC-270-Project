    "@xmldom/xmldom": "^0.8.7",
    "date-fns": "^2.29.3",
    "expo": "^48.0.10",
    "expo-blur": "^12.2.2",
    "expo-status-bar": "~1.4.4",
    "jest-babel": "^1.0.1",
    "jest-expo": "^48.0.2",
    "metro-config": "^0.76.1",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-native": "0.71.6",
    "react-native-calendars": "^1.1294.0",
